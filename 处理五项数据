def clean_data(a):
    a=str(a)
    if '未做' in a or '未查' in a or '弃查' in a:
        return np.nan
    if a.isdigit():
        return a
    else :
        try:
            return re.findall(r'^\d+\.\d+$', a)[0]
        except:
            if '>' in x:
                return a[2:]
            if '+' in x:
                i=a.index('+')
                return a[:i]
            if  len(x)>4:
               return a[0:4]

for i in target.columns[1:]:
    target[i]=target[i].apply(clean_data)
